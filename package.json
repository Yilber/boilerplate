    "gulp-rename": "^1.2.2",
    "gulp-run-command": "0.0.9",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "rollup": "^0.53.4",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "static-server": "^2.2.0"
